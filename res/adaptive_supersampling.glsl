// tolerances
uniform real MEAN_DIFF_TOL; // = 0.003
uniform real ABS_SE_TOL; // = 0.004
uniform real REL_SE_TOL; // = 0.01

// Written by ChatGPT
real evaluateWithAdaptiveSuperSampling(vec2 pixelCenter) {
    real sum = 0.0;      // sum of scalar samples
    real previousMean = 0.0;
    real m2 = 0.0;       // Welford variance accumulator

    // loop through PMJ samples progressively
    for (int i = 0; i < MAX_SAMPLES; ++i) {
        dvec2 samplePixel = dvec2(pixelCenter + offsets[i]);
        real y = evaluate(samplePixel);
        sum += y;

        // update Welford statistics
        real delta = y - previousMean;
        real meanY = previousMean + delta / real(i+1);
        real delta2 = y - meanY;
        m2 += delta * delta2;
        previousMean = meanY;

        // check convergence at stage boundaries
        if (i + 1 == 2 || i + 1 == 4 || i + 1 == 8 || i + 1 == 16) {
            real meanStage = sum / real(i+1);

            // Richardson mean change
            bool meanStable = true;
            if (i+1 > 1) {
                real dmean = abs(meanStage - previousMean);
                meanStable = (dmean <= MEAN_DIFF_TOL);
            }

            // standard error
            bool seOK = false;
            if (i+1 > 1) {
                real variance = m2 / real(i);   // sample variance
                real se = sqrt(variance / real(i+1)); // standard error
                seOK = (se <= ABS_SE_TOL) || (abs(meanStage) > 1e-12 && (se / abs(meanStage) <= REL_SE_TOL));
            } else {
                seOK = true;
            }

            if (meanStable && seOK) {
                return meanStage; // convergence achieved
            }
        }
    }

    return sum / real(MAX_SAMPLES);
}



